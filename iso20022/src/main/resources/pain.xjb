<jxb:bindings version="1.0"
	xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
	xmlns:xs="http://www.w3.org/2001/XMLSchema">

	<jxb:globalBindings>
		<jxb:javaType 
			name="javax.xml.datatype.XMLGregorianCalendar" 
			xmlType="xs:date" 
			parseMethod="org.notima.idempiere.iso20022.Iso20022FileFactory.parseISODate" 
			printMethod="org.notima.idempiere.iso20022.Iso20022FileFactory.printISODate"/>
	</jxb:globalBindings>

</jxb:bindings>